{
  "manifest_version": 2,
  "name": "Image Downloader",
  "version": "1.0",
  "description": "Download all images on a webpage",
  "permissions": ["activeTab", "downloads"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
